{
  "name": "couper",
  "displayName": "Couper Configuration",
  "version": "0.0.2",
  "publisher": "AvengaGermanyGmbH",
  "description": "Provides autocompletion, syntax/semantic validation and syntax highlighting for Couper's HCL based configuration file couper.hcl",
  "keywords": [
    "couper.hcl",
    "couper",
    "couper.io",
    "hcl"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "icon": "images/couper_logo.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "src/extension.js",
  "scripts": {
    "watch": "true"
  },
  "contributes": {
    "languages": [
      {
        "id": "couper",
        "aliases": [
          "Couper"
        ],
        "extensions": [
          ".hcl",
          ".couper"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "couper",
        "scopeName": "source.couper",
        "path": "./syntax/couper.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:couper",
    "workspaceContains:**/*.hcl"
  ],
  "devDependencies": {
    "vscode": "^1.1.37"
  },
  "bugs": "https://github.com/avenga/couper-vscode/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/avenga/couper-vscode"
  },
  "homepage": "https://github.com/avenga/couper",
  "license": "MIT"
}
